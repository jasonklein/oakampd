= form_for @venue do |f|
  = f.label :name, form_label_with_backing(:name)
  = f.text_field :name
  %br
  = f.label :address, form_label_with_backing(:address)
  = f.text_field :address
  %br
  = f.label :zipcode, form_label_with_backing(:zipcode)
  = f.text_field :zipcode
  %br
  = f.label :phone, form_label_with_backing(:phone)
  = f.text_field :phone, placeholder: "Not required; example format: 510.891.8660, NOT +1.510.891.8660"
  %br
  = f.label :url, form_label_with_backing(:url)
  = f.text_field :url, placeholder: "Not required"
  %br
  = f.submit